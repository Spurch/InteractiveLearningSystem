@model InteractiveLearningSystem.Models.School
@{
	ViewBag.Title = "Details";
	Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="row">
	<div class="well text-center">
		<h2>School Details</h2>
	</div>
</div>
<div class="row">
	<div class="col-md-3">
		@Html.Action("SchoolSnippetPartial", "SchoolsPartial", new { area = "Common", schoolId = Model.Id })
	</div>
	<div class="col-md-offset-1 col-md-8 well">
		<div class="col-md-6">
			@Html.Action("UserSnippetPartial", "UsersPartial", new { area = "Common", userId = Model.Moderator.Id })
		</div>
		<div class="col-md-6">
			@Html.Action("UserSnippetPartial", "UsersPartial", new { area = "Common", userId = Model.Consultant.Id })
		</div>
	</div>
</div>
<div class="row">

</div>

