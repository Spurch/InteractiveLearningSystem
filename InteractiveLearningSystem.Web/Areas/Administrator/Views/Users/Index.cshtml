@model IEnumerable<InteractiveLearningSystem.Web.Areas.Administrator.Models.ModeratorDetailsListView>

@{
	ViewBag.Title = "Users";
}

<div class="well text-center">
	<h2>@ViewBag.RoleName List</h2>
</div>

<div class="well">
	<table class="table">
		<tr>
			<th>
				Avatar
			</th>
		</tr>

		@foreach (var item in Model)
		{
			<tr>
				<td>
					<img src="@Url.Content(item.AvatarUrl)" style="width: 3.4em; height: 3.4em;" />
				</td>
			</tr>
		}

	</table>
</div>